<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:settings="http://schemas.android.com/apk/res/org.evolution.oneplus.DeviceExtras"
 	android.title="@string/kcal_settings_title">

    <PreferenceCategory
        android:key="kcal_values"
        android:title="@string/kcal_settings">

        <org.evolution.oneplus.DeviceExtras.preferences.RedPreference
            android:key="red"
            android:title="@string/red_title" />

        <org.evolution.oneplus.DeviceExtras.preferences.GreenPreference
            android:key="green"
            android:title="@string/green_title" />

        <org.evolution.oneplus.DeviceExtras.preferences.BluePreference
            android:key="blue"
            android:title="@string/blue_title" />

        <org.evolution.oneplus.DeviceExtras.preferences.SaturationPreference
            android:key="saturation"
            android:title="@string/saturation_title" />

        <org.evolution.oneplus.DeviceExtras.preferences.HuePreference
            android:key="hue"
            android:title="@string/hue_title" />

        <org.evolution.oneplus.DeviceExtras.preferences.ValuePreference
            android:key="value"
            android:title="@string/value_title" />

        <org.evolution.oneplus.DeviceExtras.preferences.ContrastPreference
            android:key="contrast"
            android:title="@string/contrast_title" />
    </PreferenceCategory>
</PreferenceScreen>

